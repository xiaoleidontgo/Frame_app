<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!--圆形进度View自定义属性-->
    <declare-styleable name="ShowCircleView">
        <attr name="radius_width" format="dimension" />
        <attr name="circlr_color" format="color" />
        <attr name="progress_color" format="color" />
        <attr name="stroke_width" format="dimension" />
        <attr name="text_size" format="dimension" />
        <attr name="progress" format="dimension" />
    </declare-styleable>

    <!--圆形图片-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <attr name="civ_fill_color" format="color" />
    </declare-styleable>

    <!--TabBar Tab -->
    <declare-styleable name="TabBarLayout">
        <attr name="selected_icon" format="reference" />
        <attr name="normal_icon" format="reference" />
        <attr name="selected_title_text" format="reference|string" />
        <attr name="normal_title_text" format="reference|string" />
        <attr name="title_text" format="reference|string" />
        <attr name="normal_title_color" format="color" />
        <attr name="selected_title_color" format="color" />
    </declare-styleable>

    <declare-styleable name="My_Behavior">
        <attr name="dependency_id" format="reference" />
    </declare-styleable>


    <!--ArcMenu-->
    <attr name="fromDegrees" format="float|reference" />
    <attr name="toDegrees" format="float|reference" />
    <attr name="childSize" format="dimension|reference" />

    <declare-styleable name="ArcLayout">
        <attr name="fromDegrees" />
        <attr name="toDegrees" />
        <attr name="childSize" />
    </declare-styleable>

    <declare-styleable name="ArcMenu">
        <attr name="fromDegrees" />
        <attr name="toDegrees" />
        <attr name="childSize" />
    </declare-styleable>

    <declare-styleable name="RayLayout">
        <attr name="leftHolderWidth" format="dimension" />
    </declare-styleable>



    <attr name="position">
        <enum name="left_top" value="0" />
        <enum name="right_top" value="1" />
        <enum name="right_bottom" value="2" />
        <enum name="left_bottom" value="3" />
    </attr>
    <attr name="radius" format="dimension"></attr>

    <declare-styleable name="ArcMenu2">
        <attr name="position" />
        <attr name="radius"/>
    </declare-styleable>

</resources>